<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:ModosDeFusionT.GaficosBlend"
             x:Class="ModosDeFusionT.Views.PaginaPrincipal"
             Title="PaginaPrincipal">
    <ContentPage.Resources>
        <local:Grafico x:Key="Grafico1"/>
    </ContentPage.Resources>
    <VerticalStackLayout>
        <GraphicsView Drawable="{StaticResource Grafico1}"
                      WidthRequest="500"
                      HeightRequest="500"/>
    </VerticalStackLayout>
</ContentPage>